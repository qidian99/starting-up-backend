{
  "kind": "collectionType",
  "collectionName": "companies",
  "info": {
    "name": "Company"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user"
    },
    "strategy": {
      "type": "component",
      "repeatable": false,
      "component": "company.strategy"
    },
    "fund": {
      "type": "integer",
      "default": 10,
      "min": 0,
      "required": true
    }
  }
}
